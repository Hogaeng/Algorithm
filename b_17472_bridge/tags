board	bridge.cpp	/^int board[10][10];$/;"	v
bridges	bridge.cpp	/^void bridges(){$/;"	f
check	bridge.cpp	/^bool check(vector<int> &choice){$/;"	f
cn	bridge.cpp	/^int cn;$/;"	v
color	bridge.cpp	/^void color(){$/;"	f
comb	bridge.cpp	/^int comb (int lev,int ind,vector<vector<int> > &brr, vector<vector<int> > &brc, vector<int> &choice) {$/;"	f
