debug	bridge.cpp	/^#define debug$/;"	d	file:
diff	Makefile	/^define diff$/;"	m
in_txt	Makefile	/^define in_txt$/;"	m
m	bridge.cpp	/^int m,n;$/;"	v
main	bridge.cpp	/^int main(){$/;"	f
main	test.cpp	/^int main(void){$/;"	f
